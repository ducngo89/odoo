<odoo>
    <template id="mystyle" name="My style" inherit_id="website.assets_frontend">
        <xpath expr="link[last()]" position="after">
            <link rel="stylesheet" type="text/scss" href="/ebk_ticketing/static/scss/style.scss"/>
        </xpath>
        <xpath expr="//script[last()]" position="after">
            <script type="text/javascript" src="/ebk_ticketing/static/scripts/script.js"/>
        </xpath>
    </template>
</odoo>